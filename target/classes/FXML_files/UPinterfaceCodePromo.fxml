<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.Controller.UPCodePromoController"
            prefHeight="400.0" prefWidth="600.0">
<children>
    <Label layoutX="14.0" layoutY="14.0" text="Modifier un Code Promo" />
    <Label layoutX="14.0" layoutY="55.0" text="Code :" />
    <TextField fx:id="codeField" layoutX="93.0" layoutY="51.0" prefWidth="200.0" />
    <Label layoutX="14.0" layoutY="100.0" text="Date d'expiration :" />
    <DatePicker fx:id="expirationDateField" layoutX="139.0" layoutY="96.0" prefWidth="150.0" />
    <HBox layoutX="154.0" layoutY="152.0" spacing="20.0">
        <children>
            <Button mnemonicParsing="false" onAction="#modifyCodePromo" text="Modifier" />
            <!-- <Button mnemonicParsing="false" onAction="#cancel" text="Annuler" />-->
        </children>
    </HBox>
</children>
</AnchorPane>


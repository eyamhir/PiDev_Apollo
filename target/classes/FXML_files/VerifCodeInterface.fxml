<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Controller.VerifCode">
    <children>
        <Label layoutX="315.0" layoutY="89.0" text="Reset Password">
            <font>
                <Font name="System Bold" size="28.0" />
            </font>
        </Label>
        <Button fx:id="backBT" layoutX="23.0" layoutY="377.0" mnemonicParsing="false" onAction="#back" text="Back" />
        <TextField fx:id="codeTF" layoutX="331.0" layoutY="214.0" promptText="verification code" />
        <Button fx:id="confirmBT" layoutX="372.0" layoutY="262.0" mnemonicParsing="false" onAction="#confirmCode" text="Confirm" />
    </children>
</AnchorPane>

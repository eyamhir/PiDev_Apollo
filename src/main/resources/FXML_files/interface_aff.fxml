    <children>
        <ListView fx:id="userListView" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
        <Button fx:id="modifierButton" onAction="#modifierUtilisateur" prefHeight="37.0" prefWidth="90.0" styleClass="custom-button" stylesheets="@../Values/style.css" text="Modifier" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" />
        <Button fx:id="supprimerButton" onAction="#supprimerUtilisateur" styleClass="custom-button" stylesheets="@../Values/style.css" text="Supprimer" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />
    </children>
</AnchorPane>
